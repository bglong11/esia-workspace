# Get your key from aistudio.google.com
GOOGLE_API_KEY=your_api_key_here

# Get your key from openrouter.ai
OPENROUTER_API_KEY=your_openrouter_key_here

# Get your xAI API key from console.x.ai
XAI_API_KEY=your_xai_key_here

# Google Cloud Project ID for GCS operations
GOOGLE_CLOUD_PROJECT=your_project_id_here

# Default LLM provider for DSPy: "google", "openrouter", or "xai"
LLM_PROVIDER=google

# Default model names
GOOGLE_MODEL=gemini-2.0-flash-exp
OPENROUTER_MODEL=anthropic/claude-3.5-sonnet
XAI_MODEL=grok-3-mini

# Available xAI models:
# - grok-4: Best quality, 256K context
# - grok-4-fast: Fast inference, 2M context
# - grok-3: Good quality, 131K context
# - grok-3-mini: Fastest, 131K context, cost-effective
